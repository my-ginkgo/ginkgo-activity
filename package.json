{
  "name": "@my-ginkgo/ginkgo-metrics-lib",
  "version": "0.0.6",
  "private": false,
  "description": "TS Library for metrics generation for Ginkgo.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"lib/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json --fix",
    "release": "npm version patch && npm run build && npm publish && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/my-ginkgo/ginkgo-metrics-lib.git"
  },
  "keywords": [
    "metricsai",
    "ginkgo"
  ],
  "author": "myGinkgo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/my-ginkgo/ginkgo-metrics-lib/issues"
  },
  "homepage": "https://github.com/my-ginkgo/ginkgo-metrics-lib#readme",
  "devDependencies": {
    "@types/leaflet": "^1.9.0",
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  },
  "dependencies": {
    "leaflet": "^1.9.2"
  }
}
